{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "project": ["src/**", "tests/**", "scripts/**"],
  "entry": [
    "src/index.ts",
    "src/Runtime/*.ts"
  ],
  "ignore":["tests/**/__snapshots__/**", "scripts/defaultChromeWindow.js", "src/types/SteamClient/Notifications.ts", "src/types/Protobufs/**"],
  "ignoreExportsUsedInFile": {
    "interface": true,
    "enum": true,
    "type": true
  },
  "ignoreDependencies": [
    "@semantic-release/github",
    "@semantic-release/npm",
    "@steambrew/client"
  ]
}
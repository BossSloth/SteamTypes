{
  "name": "steam-types",
  "version": "0.1.4",
  "author": "BossSloth",
  "description": "TypeScript type definitions for the Steam client",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "watch": "tsc -w --project tsconfig.build.json",
    "build-convert-debug": "esbuild scripts/convert-to-typescript/test.ts --bundle --format=iife --platform=browser --outfile=build/scripts/convert-to-typescript.js",
    "build-convert": "bun build-convert-debug --minify-identifiers --minify-syntax",
    "validate-types": "bun scripts/validate-types.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "knip": "knip",
    "lint": "knip ; eslint",
    "lint:fix": "knip --fix ; eslint --fix",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "steam",
    "types",
    "typescript",
    "definitions",
    "interface",
    "valve"
  ],
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@semantic-release/git": "^10.0.1",
    "@stylistic/eslint-plugin": "^4.4.1",
    "@types/chrome-remote-interface": "^0.31.14",
    "@types/node": "^22.18.5",
    "@vitest/coverage-v8": "^3.2.4",
    "ansi-regex": "^6.2.2",
    "chalk": "^5.6.2",
    "chrome-remote-interface": "^0.33.3",
    "commander": "^14.0.1",
    "dedent": "^1.7.0",
    "diff": "^8.0.2",
    "esbuild": "^0.25.10",
    "eslint": "^9.35.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "globals": "^16.4.0",
    "knip": "^5.63.1",
    "semantic-release": "^24.2.8",
    "ts-morph": "27.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@types/google-protobuf": "^3.15.12",
    "@types/react-dom": "^19.1.9",
    "long": "^5.3.2",
    "mobx": "^6.13.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BossSloth/SteamTypes.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "patchedDependencies": {
    "@ts-morph/common@0.28.0": "patches/@ts-morph%2Fcommon@0.28.0.patch"
  }
}

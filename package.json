{
  "name": "steam-types",
  "version": "1.0.0",
  "author": "BossSloth",
  "description": "TypeScript type definitions for the Steam client",
  "types": "dist/index.d.ts",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test-convert": "bun scripts/convert/test-convert.ts",
    "convert": "bun scripts/convert/convert-to-ts.ts",
    "build-convert": "esbuild scripts/convert/convert-to-ts.ts --bundle --format=iife --platform=browser --minify-syntax --outfile=dist/scripts/convert-to-ts.js"
  },
  "keywords": [
    "steam",
    "types",
    "typescript"
  ],
  "license": "MIT",
  "devDependencies": {
    "@inquirer/prompts": "^7.3.2",
    "@types/chrome-remote-interface": "^0.31.14",
    "@types/diff": "^7.0.1",
    "@types/node": "^22.13.5",
    "chalk": "^5.4.1",
    "chrome-remote-interface": "^0.33.3",
    "commander": "^13.1.0",
    "diff": "^7.0.0",
    "esbuild": "^0.25.0",
    "ts-morph": "^25.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}

{
  "name": "steam-types",
  "version": "0.1.1",
  "author": "BossSloth",
  "description": "TypeScript type definitions for the Steam client",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "watch": "tsc -w --project tsconfig.build.json",
    "convert": "bun $1 scripts/convert-to-typescript/test.ts",
    "build-convert-debug": "esbuild scripts/convert-to-typescript/test.ts --bundle --format=iife --platform=browser --minify-syntax --outfile=build/scripts/convert-to-typescript.js",
    "build-convert": "bun build-convert-debug --minify-identifiers",
    "validate-types": "bun scripts/validate-types.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "knip": "knip"
  },
  "keywords": [
    "steam",
    "types",
    "typescript",
    "definitions",
    "interface",
    "valve"
  ],
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@semantic-release/git": "^10.0.1",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@types/chrome-remote-interface": "^0.31.14",
    "@types/diff": "^7.0.1",
    "@types/node": "^22.13.14",
    "@vitest/coverage-v8": "^3.0.9",
    "ansi-regex": "^6.1.0",
    "chalk": "^5.4.1",
    "chrome-remote-interface": "^0.33.3",
    "commander": "^13.1.0",
    "dedent": "^1.5.3",
    "diff": "^7.0.0",
    "esbuild": "^0.25.1",
    "eslint": "^9.22.0",
    "eslint-plugin-perfectionist": "^4.10.1",
    "globals": "^16.0.0",
    "knip": "^5.46.3",
    "semantic-release": "^24.2.3",
    "ts-morph": "^25.0.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vitest": "^3.0.9"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "long": "^5.3.1",
    "mobx": "^6.13.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BossSloth/SteamTypes.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
